{
  "name": "notes-cli",
  "version": "1.0.0",
  "description": "A quick way to takes notes through command line.",
  "main": "dist/index.js",
  "bin": {
    "mn": "./dist/bin/mn.js"
  },
  "scripts": {
    "dev": "ts-node src/bin/mn.ts",
    "build": "rimraf dist && tsc",
    "build:fast": "esbuild src/bin/mn.ts --bundle --platform=node --outfile=dist/bin/mn.js",
    "watch": "tsc --watch",
    "start": "node dist/bin/mn.js",
    "lint": "eslint src --ext .ts",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "notes",
    "cli",
    "typescript",
    "knowledge"
  ],
  "author": "SATYAM1000",
  "license": "ISC",
  "type": "commonjs",
  "devDependencies": {
    "@types/fs-extra": "^11.0.4",
    "@types/glob": "^8.1.0",
    "@types/inquirer": "^9.0.9",
    "@types/node": "^24.10.1",
    "esbuild": "^0.27.0",
    "nodemon": "^3.1.11",
    "rimraf": "^6.1.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "@inquirer/prompts": "^8.0.1",
    "boxen": "^8.0.1",
    "chalk": "^5.6.2",
    "cli-highlight": "^2.1.11",
    "cli-table3": "^0.6.5",
    "commander": "^14.0.2",
    "date-fns": "^4.1.0",
    "fs-extra": "^11.3.2",
    "glob": "^13.0.0",
    "gray-matter": "^4.0.3",
    "inquirer": "^13.0.1",
    "marked": "^17.0.1",
    "ora": "^9.0.0",
    "simple-git": "^3.30.0",
    "zod": "^4.1.12"
  }
}
